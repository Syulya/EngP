.eng-main__title 
	text-align center
.eng-main-content
	font-size 22px
.eng-item
	font-size 22px
	border 1px solid black
	margin-bottom 15px
	padding 10px
	
.eng-prepos
	overflow hidden
.eng-prep-cont__title,
.eng-item__title
	font-size 26px
	font-weight bold
.eng-prep-cont
	float left
.eng-prep
	float right
	span
		font-size 30px
		margin-right 30px
		font-weight bold
		text-shadow 3px 3px 3px #6c6b70
		display inline-block
.on
	color yellow
	transform rotate(30deg)	
.about
	color red
	transform rotate(-50deg)	
.for
	color green
	transform rotate(40deg)
.at
	color blue
	transform rotate(20deg)
.in
	color #792794
	transform rotate(-40deg)
.with
	color #e081cb
	transform rotate(60deg)